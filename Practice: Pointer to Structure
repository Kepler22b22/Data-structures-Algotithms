
#include <iostream>
using namespace std;

struct Rectangle
{
    int length;
    int breadth;
};

int main()
{
    Rectangle *p;
    p=new Rectangle;  //use new to creat in heap memory
    
    p->length=15;
    p->breadth=7;
    
    cout<<p->length<<endl;
    cout<<p->breadth<<endl;

    return 0;
}
