
#include <iostream>  
using namespace std; 


int fun(int n)
{
    
    if(n>0)
    {
        
        return n+fun(n-1);
    }
    return 0;
}

int main()
{

    int r;
    r=fun(5);
    cout<<r<<endl;          // = 25
    
    int r;
    r=fun(5);
    cout<<r<<endl;          // = 50

    return 0;
}
